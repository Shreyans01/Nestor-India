{
  "name": "nirmalshree",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "stats": "ng build --stats-json",
    "web-pack": "webpack-bundle-analyzer dist/nirmalshree/stats.json",
    "test": "ng test",
    "dev-ssr": "ng run nirmalshree:serve-ssr",
    "serve:ssr": "node dist/nirmalshree/server",
    "build:ssr": "ng build && ng run nirmalshree:server",
    "build:ssr1": "ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path && ng run nirmalshree:server",
    "build:optimize": "ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer && ng run nirmalshree:server",
    "prerender": "ng run nirmalshree:prerender",
    "build-optimize-seo":"ng build --prod --stats-json",

    "latest-build-command":"ng build --prod --stats-json"
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "^12.2.13",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.12",
    "@angular/compiler": "^12.2.12",
    "@angular/core": "^12.2.12",
    "@angular/forms": "^12.2.12",
    "@angular/localize": "^12.2.12",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.12",
    "@angular/platform-browser-dynamic": "^12.2.12",
    "@angular/platform-server": "^12.2.12",
    "@angular/router": "^12.2.12",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ng-select/ng-select": "^7.4.0",
    "@nguniversal/express-engine": "^12.1.3",
    "@types/crypto-js": "^4.0.2",
    "@types/node-forge": "^0.10.8",
    "angular-datatables": "^13.0.1",
    "angular-ng-autocomplete": "^2.0.5",
    "angular-responsive-carousel": "^2.1.2",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.6.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "datatables.net": "^1.11.3",
    "datatables.net-dt": "^1.11.3",
    "dayjs": "^1.10.7",
    "express": "^4.15.2",
